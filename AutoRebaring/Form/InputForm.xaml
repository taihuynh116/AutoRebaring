                                        <TextBox x:Name="txtStep" HorizontalAlignment="Left" Height="23" Margin="0,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <Label Content="Thép cắm chờ:" FontWeight="Bold" ></Label>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Phối thép chuẩn:"></Label>
                                        <Label Content="Lmax:" Margin="15,0,0,0"></Label>
                                        <TextBox x:Name="txtImplantLmax" HorizontalAlignment="Left" Height="23" Margin="0,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="L ="></Label>
                                        <TextBox x:Name="txtImplantL0" HorizontalAlignment="Left" Height="23" Margin="52,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtImplantL1" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtImplantL2" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtImplantL3" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtImplantL4" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtImplantL5" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
