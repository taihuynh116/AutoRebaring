                                    <Label Content="Thép đai:" FontWeight="Bold" ></Label>
                                    <Label Content="Loại family:" ></Label>
                                    <StackPanel Orientation="Vertical">
                                        <StackPanel Orientation="Horizontal">
                                            <Label Content="Đai Bao:"></Label>
                                            <ComboBox x:Name="cbbFamilyDaiBao" HorizontalAlignment="Left" Height="20" Margin="10,0,0,0" VerticalAlignment="Center" Width="130"/>
                                        </StackPanel>
                                        <StackPanel x:Name="spDaiBien" Orientation="Horizontal">
                                            <Label x:Name="lblDaiBien" Content="Đai Biên:" Margin="0,0,0,0"></Label>
                                            <ComboBox x:Name="cbbFamilyDaiBien" HorizontalAlignment="Left" Height="20" Margin="7,0,0,0" VerticalAlignment="Center" Width="130"/>
                                        </StackPanel>
                                        <StackPanel Orientation="Horizontal">
                                            <Label Content="Đai C:" Margin="0,0,0,0"></Label>
                                            <ComboBox x:Name="cbbFamilyDaiC" HorizontalAlignment="Left" Height="20" Margin="23,0,0,0" VerticalAlignment="Center" Width="130"/>
