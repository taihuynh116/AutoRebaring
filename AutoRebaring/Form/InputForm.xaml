<UserControl x:Class="AutoRebaring.Form.InputForm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutoRebaring.Form"
             mc:Ignorable="d" Width="1140" Height="1557">
    <Grid>
        <ScrollViewer Margin="0,0,0,0" VerticalScrollBarVisibility="Visible">
            <StackPanel Orientation="Vertical">
                <StackPanel Orientation="Horizontal">
                    <GroupBox Header="ĐĂNG NHẬP" HorizontalAlignment="Left" Height="100" Margin="0,0,0,0" VerticalAlignment="Top" Width="327" FontWeight="Normal">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Tên dự án:"></Label>
                                    <Label x:Name="lblProject" Content="Bêtông bảo vệ:" HorizontalAlignment="Right" TextBlock.TextAlignment="Right" Margin="88,0,0,0" VerticalAlignment="Top"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Tên đăng nhập:"></Label>
                                    <TextBox x:Name="txtUserName" Margin="53,0,0,0" TextAlignment="Right" HorizontalAlignment="Left" Height="23" TextWrapping="NoWrap"  Text="" VerticalAlignment="Top" Width="100"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Mật khẩu:"></Label>
                                    <PasswordBox x:Name="txtPassword" Margin="81,0,0,0" TextBlock.TextAlignment="Right" HorizontalAlignment="Left" VerticalAlignment="Top" PasswordChar="*" Width="100" Height="23"/>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <Button x:Name="btnLogout" Content="Log out" Height="25"  HorizontalAlignment="Center" Margin="15,10,0,7" VerticalAlignment="Bottom" Width="47" Click="btnLogout_Click"/>
                                <Button x:Name="btnCheckUser" Content="OK" Height="25" HorizontalAlignment="Center" Margin="15,0,0,7" VerticalAlignment="Bottom" Width="47" Click="btnCheckUser_Click"/>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox x:Name="colParamGrb" Header="THAM BIẾN " HorizontalAlignment="Left" Margin="0,0,0,0" VerticalAlignment="Top"  FontWeight="Normal">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal" Margin="0,0,0,0">
                                    <Label Content="Mark:" Width="39"></Label>
                                    <ComboBox x:Name="cbbMark" IsEditable="True" Width="74" HorizontalAlignment="Left" Height="22" Margin="10,0,0,0" Text="" VerticalAlignment="Center" />
                                    <Button x:Name="btnLoad" Margin="15,0,0,0" Width="40" Content="Load" Click="btnLoad_Click"></Button>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,0,0,0">
                                    <Label Content="Type:" Width="39"></Label>
                                    <RadioButton x:Name="rbtColumn" VerticalAlignment="Center" Content="Column" Margin="10,0,0,0" Checked="rbtColumn_Checked"/>
                                    <RadioButton x:Name="rbtWall" VerticalAlignment="Center" Content="Wall" Margin="15,0,0,0" Checked="rbtColumn_Checked"></RadioButton>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>
                    <GroupBox x:Name="wallParamGrb" Header="THAM BIẾN VÁCH">
                        <StackPanel Orientation="Vertical">
                            <Label Content="Include" Margin="200,-5,0,0" FontWeight="Bold"></Label>
                            <StackPanel Orientation="Horizontal">
                                <Label Content="Bề rộng biên:"></Label>
                                <TextBox x:Name="txtEdgeWidth" Margin="25,0,0,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38" IsEnabled="False"/>
                                <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                <CheckBox x:Name="chkEdgeWidth" Margin="43,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkEdgeWidth_Checked" Unchecked="chkEdgeWidth_Checked"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Label Content="L/" Margin="118,0,0,0"></Label>
                                <TextBox x:Name="txtEdgeRatio" Margin="2,0,0,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                <CheckBox x:Name="chkEdgeRatio" Margin="51,0,0,0" Content="" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkEdgeRatio_Checked" Unchecked="chkEdgeRatio_Checked"/>
                            </StackPanel>
                        </StackPanel>
                    </GroupBox>
                </StackPanel>
                <StackPanel Orientation="Horizontal">
                    <StackPanel Orientation="Vertical">
                        <GroupBox x:Name="genParamGrb" Header="THÔNG SỐ CHUNG" HorizontalAlignment="Left" Height="356" Margin="0,0,0,0" VerticalAlignment="Top" Width="373" FontWeight="Normal">
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Orientation="Vertical">
                                    <Label Content="Include" Margin="315,-5,0,0" FontWeight="Bold"></Label>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Bêtông bảo vệ:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtConcCover" VerticalAlignment="Center" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Chiều dài nối thép:" HorizontalAlignment="Left"  VerticalAlignment="Top" FontWeight="Normal"/>
                                        <TextBox x:Name="txtDevMulti" HorizontalAlignment="Left" Height="23" VerticalAlignment="Center" TextWrapping="Wrap" Text="" TextAlignment="Right" Width="27" FontWeight="Normal"/>
                                        <Label Content="d" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Khoảng cách các đoạn nối:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtDevsDist"  HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Thép đai gia cường vùng nối:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <CheckBox x:Name="chkReinfStirrInclude" VerticalAlignment="Center" Margin="168,0,0,0"></CheckBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Nối phạm vùng dầm:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <Label Content="Delta:" HorizontalAlignment="Left" VerticalAlignment="Top" Width="42"/>
                                        <TextBox x:Name="txtDelDevError"  HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <Label Content="Số lần" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtNumDevError"  HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                        <CheckBox x:Name="chkDevErrorInclude" VerticalAlignment="Center" Margin="41,0,0,0" Checked="chkDevErrorInclude_Checked" Unchecked="chkDevErrorInclude_Checked"></CheckBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Vị trí nối thép cách level:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtDevLevelOffAllow"  HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="36" IsEnabled="False"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <CheckBox x:Name="chkDevLevelOffInclude" VerticalAlignment="Center" Margin="125,0,0,0" Checked="chkDevLevelOffInclude_Checked" Unchecked="chkDevLevelOffInclude_Checked"></CheckBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Bóp tiết diện giới hạn:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtShortenLimit" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Chiều dài neo thép:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtAncMulti" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27"/>
                                        <Label Content="d" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Chiều dài khóa đầu:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtLockHeadMulti" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27"/>
                                        <Label Content="d" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Khoảng cách mép sàn tới vị trí khóa đầu:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtConcTopCover" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Tỉ lệ LH thép bẻ ke:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtRatioLH" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="23"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Khoảng cách mép sàn đến vị trí bẻ ke:" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                        <TextBox x:Name="txtCoverTopSmall" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    </StackPanel>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox x:Name="devRebarGrb" Header="VỊ TRÍ NỐI THÉP" Height="213" Width="402" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,0,0,0">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Thép dọc" Margin="105,0,0,0"></Label>
                                    <Label Content="Include" Margin="11,0,0,0" FontWeight="Bold"></Label>
                                    <Label Content="Thép đai" Margin="50,0,0,0"></Label>
                                    <Label Content="Include" Margin="11,0,0,0" FontWeight="Bold"></Label>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="Bottom Offset:"></Label>
                                    <TextBox x:Name="txtBotOff" Margin="12,0,0,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38" IsEnabled="False"/>
                                    <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    <CheckBox x:Name="chkOff" Margin="22,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkOff_Checked" Unchecked="chkOff_Checked"/>
                                    <TextBox x:Name="txtBotOffStir" Margin="62,0,0,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Top" Width="38" IsEnabled="False"/>
                                    <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    <CheckBox x:Name="chkOffStir" HorizontalAlignment="Left" Margin="22,0,0,0" VerticalAlignment="Center" Checked="chkOffStir_Checked" Unchecked="chkOffStir_Checked"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="L/" Margin="110,0,0,0"></Label>
                                    <TextBox x:Name="txtBotOffRatio" Margin="2,0,0,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                    <CheckBox x:Name="chkOffRatio" Margin="32,0,0,0" Content="" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkOffRatio_Checked" Unchecked="chkOffRatio_Checked"/>
                                    <Label Content="L/" Margin="66,0,0,0"></Label>
                                    <TextBox x:Name="txtBotOffRatioStir" HorizontalAlignment="Left" Height="23" Margin="2,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                    <CheckBox x:Name="chkOffRatioStir"  Margin="32,0,0,0" Content="" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkOffRatioStir_Checked" Unchecked="chkOffRatioStir_Checked"/>
                                </StackPanel>
                                <StackPanel Margin="0,20,0,0" Orientation="Horizontal">
                                    <Label Content="Top Offset:"></Label>
                                    <TextBox x:Name="txtTopOff" Margin="32,0,0,0" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38" IsEnabled="False"/>
                                    <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    <CheckBox x:Name="chkOff2" Margin="22,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkOff_Checked" Unchecked="chkOff_Checked"/>
                                    <TextBox x:Name="txtTopOffStir" HorizontalAlignment="Left" Height="23" Margin="62,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Top" Width="38" IsEnabled="False"/>
                                    <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                                    <CheckBox x:Name="chkOffStir2" HorizontalAlignment="Left" Margin="22,0,0,0" VerticalAlignment="Center" Checked="chkOffStir_Checked" Unchecked="chkOffStir_Checked"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal">
                                    <Label Content="L/" Margin="110,0,0,0"></Label>
                                    <TextBox x:Name="txtTopOffRatio" HorizontalAlignment="Left" Height="23" Margin="2,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                    <CheckBox x:Name="chkOffRatio2" Margin="32,0,0,0" Content="" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkOffRatio_Checked" Unchecked="chkOffRatio_Checked"/>
                                    <Label Content="L/" Margin="66,0,0,0"></Label>
                                    <TextBox x:Name="txtTopOffRatioStir" HorizontalAlignment="Left" Height="23" Margin="2,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="27" IsEnabled="False"/>
                                    <CheckBox x:Name="chkOffRatioStir2" Margin="32,0,0,0" Content="" HorizontalAlignment="Left" VerticalAlignment="Center" Checked="chkOffRatioStir_Checked" Unchecked="chkOffRatioStir_Checked"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,18,0,0">
                                    <Label Content="Bỏ qua cao độ dầm"></Label>
                                    <CheckBox x:Name="chkInsideBeam" HorizontalAlignment="Left" Margin="78,0,0,0" VerticalAlignment="Center"/>
                                    <CheckBox x:Name="chkStirInsideBeam" HorizontalAlignment="Left" Margin="150,0,0,0" VerticalAlignment="Center"/>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                    </StackPanel>
                    <StackPanel Orientation="Vertical">
                        <StackPanel Orientation="Horizontal">
                            <GroupBox x:Name="rebarChosenGrb" Header="CHỌN THÉP" Width="467" Margin="0,0,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">
                                <StackPanel Orientation="Vertical">
                                    <Label Content="Thép dọc:" FontWeight="Bold" ></Label>
                                    <Label Content="Phối thép chuẩn:"></Label>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="L ="></Label>
                                        <TextBox x:Name="txtFitL0" HorizontalAlignment="Left" Height="23" Margin="52,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL1" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL2" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL3" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL4" HorizontalAlignment="Left" Height="22" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL5" HorizontalAlignment="Left" Height="22" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL6" HorizontalAlignment="Left" Height="22" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
