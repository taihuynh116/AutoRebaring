                                        <TextBox x:Name="txtLmin" HorizontalAlignment="Left" Height="23" Margin="0,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
                                        <Label Content="mm" HorizontalAlignment="Left" VerticalAlignment="Top"/>
