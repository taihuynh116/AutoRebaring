                                        <TextBox x:Name="txtFitL33" HorizontalAlignment="Left" Height="23" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL34" HorizontalAlignment="Left" Height="22" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL35" HorizontalAlignment="Left" Height="22" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                        <TextBox x:Name="txtFitL36" HorizontalAlignment="Left" Height="22" Margin="5,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="49"/>
                                    </StackPanel>
                                    <Label Content="Phối thép dư:"></Label>
                                    <StackPanel Orientation="Horizontal">
                                        <Label Content="Lmax:"></Label>
                                        <TextBox x:Name="txtLmax" HorizontalAlignment="Left" Height="23" Margin="0,0,0,0" TextWrapping="Wrap" Text="" TextAlignment="Right" VerticalAlignment="Center" Width="38"/>
